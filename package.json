{
  "name": "ignite-node-rentx",
  "version": "1.0.0",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --respawn --transpile-only --ignore-watch node_modules src/server.ts",
    "build": "tsup src/server.ts --minify",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migrate": "pnpm typeorm migration:run -- -d ./src/database/index.ts"
  },
  "dependencies": {
    "csv-parse": "^5.5.6",
    "express": "^4.21.0",
    "multer": "1.4.5-lts.1",
    "pg": "^8.13.0",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.1",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/express": "4.17.13",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "eslint": "8.56.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=20"
  }
}